{
    "name": "custom-diagram-boilerplate",
    "version": "0.1.0",
    "private": true,
    "devDependencies": {
        "@parcel/optimizer-data-url": "2.9.3",
        "@parcel/transformer-inline-string": "2.9.3",
        "@parcel/transformer-sass": "2.9.3",
        "@parcel/core": "2.9.3",
        "parcel": "2.9.3",
        "posthtml-include": "1.7.4"
    },
    "targets": {
        "default": {
            "distDir": "./output"
        }
    },
    "browserslist": [
        "defaults",
        "last 2 years",
        "not dead"
    ],
    "scripts": {
        "start": "parcel clearcalcs/**/index.html",
        "test": "parcel clearcalcs/**/*.html --port 4321",
        "compile": "parcel build clearcalcs/**/index.html",
        "docs": "cd docs && npm install && docsify serve -p 4444"
    }
}
